{
  "name": "tesseract-api",
  "version": "1.0.2",
  "description": "OCR Microservice using Tesseract",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "version:check": "node scripts/release/version-check.js",
    "release:status": "node scripts/release/release-status.js",
    "version:patch": "npm version patch -m \"Patching to v%s\"",
    "version:minor": "npm version minor -m \"Progressing to v%s\"",
    "version:major": "npm version major -m \"Evolving to v%s\"",
    "version:interactive": "node scripts/release/version-interactive.js",
    "push:dev": "npm run version:patch && node scripts/release/git-push.js dev",
    "push:staging": "npm run version:interactive && node scripts/release/git-push.js staging",
    "push:prod": "npm run version:interactive && node scripts/release/git-push.js prod"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.22",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
